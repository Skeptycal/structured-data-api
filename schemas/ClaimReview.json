{
  "$schema": "http://json-schema.org/schema#",
  "title": "ClaimReview",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "url": {
      "type": "string",
      "format": "uri"
    },
    "image": {
      "type": "string",
      "format": "uri"
    },
    "dateCreated": {
      "type": "string",
      "format": "date-time"
    },
    "dateModified": {
      "type": "string",
      "format": "date-time"
    },
    "mainEntity": {
      "$ref": "Thing.json"
    },
    "mentions": {
      "type": "array",
      "items": {
        "$ref": "Thing.json"
      }
    },
    "claimReviewed": {
      "type": "string"
    },
    "reviewBody": {
      "type": "string"
    },
    "itemReviewed": {
      "type": "string",
      "format": "uri"
    },
    "reviewRating": {
      "$ref": "Rating.json"
    },
    "temporalCoverage": {
      "type": "string",
      "format": "date-time"
    },
    "spatialCoverage": {
      "$ref": "Place.json"
    },
    "citation": {
      "type": "string"
    }
  },
  "additionalProperties": false
}